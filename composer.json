{
  "name": "nhagemann/anycontent-php",
  "description": "Client library to work with AnyContent repositories. Provides connectors for different repository types (file based, mysql, rest).",
  "type": "library",
  "homepage": "http://www.nilshagemann.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Nils Hagemann",
      "email": "mail@nilshagemann.de",
      "homepage": "http://www.nilshagemann.de"
    }
  ],
  "require": {
    "php": ">=5.4",
    "nhagemann/cmdl-parser-php": "~3.0",
    "psr/log": "1.*",
    "symfony/filesystem": "~2.3|~3.0",
    "symfony/finder": "~2.3|~3.0",
    "guzzlehttp/guzzle": "~5.0",
    "doctrine/cache": "~1.5",
    "nhagemann/kvmlogger": "~1.0",
    "imagine/imagine": "~0.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~5",
    "aws/aws-sdk-php": "~2.8",
    "dflydev/apache-mime-types": "~1.0",
    "cpliakas/git-wrapper": "~1.0",
    "ktamas77/firebase-php": "~2.0"
  },
  "autoload": {
    "psr-0": {
      "AnyContent": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}